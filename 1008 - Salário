#include <stdio.h>

int main() {
    int numero, horas_trabalhadas;
    double valor_por_hora, salario;
    
    scanf("%d", &numero);
    scanf("%d", &horas_trabalhadas);
    scanf("%lf", &valor_por_hora);
    
    salario = horas_trabalhadas * valor_por_hora;
    
    printf("NUMBER = %d\n", numero);
    printf("SALARY = U$ %.2lf\n", salario);
    
    return 0;
}
