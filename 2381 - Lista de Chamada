#include <stdio.h>
#include <string.h>

#define MAX_ALUNOS 100
#define MAX_NOME 21

int main() {
    int n, k;
    char alunos[MAX_ALUNOS][MAX_NOME];
    char nome_selecionado[MAX_NOME];

    scanf("%d %d", &n, &k);

    for (int i = 0; i < n; i++) {
        scanf("%s", alunos[i]);
    }

    // Ordenar os nomes em ordem alfabÃ©tica
    for (int i = 0; i < n - 1; i++) {
        for (int j = i + 1; j < n; j++) {
            if (strcmp(alunos[i], alunos[j]) > 0) {
                char temp[MAX_NOME];
                strcpy(temp, alunos[i]);
                strcpy(alunos[i], alunos[j]);
                strcpy(alunos[j], temp);
            }
        }
    }

    printf("%s\n", alunos[k - 1]);

    return 0;
}
